    \u0441\u0442\u043E\u0440\u043E\u043D\u044B.  \u041C\u0435\u043B\u043E\u0447\u043D\u044B\u0439
    \u0438 \u0437\u043B\u043E\u0439 \u0447\u0435\u043B\u043E\u0432\u0435\u043A \u043E\u0431\u044B\u0447\u043D\u043E
    \u0438\u043C\u0435\u0435\u0442 \u0442\u0435\u043D\u0434\u0435\u043D\u0446\u0438\u044E
    \u043A \u043F\u043E\u0434\u0447\u0435\u0440\u043A\u0438\u0432\u0430\u043D\u0438\u044E
    \u043D\u0435\u0434\u043E\u0441\u0442\u0430\u0442\u043A\u043E\u0432, \u043A\u0440\u0438\u0442\u0438\u043A\u0435
    \u043E\u043A\u0440\u0443\u0436\u0430\u044E\u0449\u0438\u0445, \u0430 \u0442\u0430\u043A\u0436\u0435
